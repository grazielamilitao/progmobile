<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:contextClickable="false"
    tools:context=".LancamentoActivity"
    tools:layout_editor_absoluteX="-73dp"
    tools:layout_editor_absoluteY="-2dp">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cod"
        app:layout_constraintTop_toTopOf="@+id/codQtdComp"
        tools:layout_editor_absoluteX="9dp" />

    <EditText
        android:id="@+id/codQtdComp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="26dp" />

    <TextView
        android:id="@+id/tvQuantidadeLancamento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Qtd:"
        app:layout_constraintTop_toTopOf="@+id/qtdLancamento"
        tools:layout_editor_absoluteX="8dp" />

    <EditText
        android:id="@+id/qtdLancamento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="92dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Valor"
        app:layout_constraintTop_toTopOf="@+id/valorlancamento"
        tools:layout_editor_absoluteX="9dp" />

    <EditText
        android:id="@+id/valorlancamento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="160dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="btConfirmarOnClick"
        android:text="Confirmar"
        tools:layout_editor_absoluteX="61dp"
        tools:layout_editor_absoluteY="241dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="btListarOnClick"
        android:text="Lispar"
        app:layout_constraintBottom_toBottomOf="@+id/button"
        tools:layout_editor_absoluteX="238dp" />

</androidx.constraintlayout.widget.ConstraintLayout>